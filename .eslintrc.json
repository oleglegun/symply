{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "google", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "spaced-comment": "off",
        "require-jsdoc": "off"
    }
}
