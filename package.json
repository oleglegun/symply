{
    "name": "symply",
    "version": "0.6.2",
    "description": "A simple static site generator.",
    "author": "Oleg Legun <oleg.legun@gmail.com>",
    "homepage": "https://github.com/oleglegun/symply#readme",
    "repository": {
        "type": "git",
        "url": "git+https://https://github.com/oleglegun/symply.git"
    },
    "bugs": {
        "url": "https://github.com/oleglegun/symply/issues"
    },
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "compile": "npm run dist:clear && npx tsc",
        "publish-and-push": "npm run compile && npm publish && git tag \"v$npm_package_version\" && git push && git push --tags",
        "dist:clear": "rm -rf dist",
        "start": "node index.js",
        "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
        "cloc": "cloc --exclude-dir=node_modules,dist --exclude-ext=json .",
        "test": "npm run compile && tape test/**/*.js | tap-spec"
    },
    "dependencies": {
        "chalk": "4.1.2",
        "flat": "5.0.2",
        "fs-extra": "10.0.1",
        "handlebars": "4.7.7",
        "html-minifier": "4.0.0",
        "js-yaml": "4.1.0",
        "lodash": "4.17.21",
        "progress": "2.0.3",
        "sass": "1.49.9",
        "strip-ansi": "6.0.1"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/flat": "5.0.2",
        "@types/fs-extra": "9.0.13",
        "@types/html-minifier": "4.0.2",
        "@types/js-yaml": "4.0.5",
        "@types/lodash": "4.14.179",
        "@types/node": "17.0.21",
        "@types/prettier": "2.4.4",
        "@types/progress": "2.0.5",
        "@types/sass": "1.43.1",
        "@typescript-eslint/eslint-plugin": "5.14.0",
        "@typescript-eslint/parser": "5.14.0",
        "eslint": "8.11.0",
        "eslint-config-google": "0.14.0",
        "eslint-config-prettier": "8.5.0",
        "prettier": "2.5.1",
        "tap-spec": "^5.0.0",
        "tape": "5.5.2",
        "typescript": "4.6.2"
    },
    "overrides": {
        "trim": "1.0.1"
    },
    "types": "./index.d.ts"
}
